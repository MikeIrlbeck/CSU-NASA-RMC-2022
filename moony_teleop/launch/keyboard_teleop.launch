<launch>
  <!-- moony_teleop_key already has its own built in velocity smoother -->
  <node pkg="moony_teleop" type="moony_teleop_key.py" name="moony_teleop_keyboard" output="screen">
    <param name="scale_linear" value="0.5" type="double"/>
    <param name="scale_angular" value="1.5" type="double"/>
    <remap from="moony_teleop_keyboard/cmd_vel" to="/moony/cmd_vel"/>   <!-- cmd_vel_mux/input/teleop"/-->
  </node>
</launch>